{
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@tailwindcss/cli": "^4.1.11",
    "aos": "^2.3.4",
    "notyf": "^3.10.0",
    "swiper": "^11.2.10",
    "tailwindcss": "^4.1.11"
  },
  "scripts": {
     "dev:css": "npx @tailwindcss/cli -i ./src/input.css -o ./src/output.css --watch",
    "dev:server": "http-server . -p 3001 --silent",
    "dev": "concurrently --kill-others-on-fail \"npm run dev:css\" \"npm run dev:server\" --prefix-colors \"blue.bold,green.bold\" --name \"CSS,SERVER\" --hide-prefix-colors"
},
